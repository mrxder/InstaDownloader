{
    "name": "InstaDownloader",
    "version": "0.1",
    "description": "Downloader for Instagram",
    "background": {
        "scripts": ["background.js"],
        "persistent": false
    },
    "permissions": [
        "tabs",
        "contextMenus",
        "downloads",
        "https://www.instagram.com/*"
    ],
    "icons": {
        "16": "logo/insta_dl_logo_16.png",
        "32": "logo/insta_dl_logo_32.png",
        "48": "logo/insta_dl_logo_48.png",
        "128": "logo/insta_dl_logo_128.png"
    },
    "manifest_version": 2
}
